{% extends reg_get('output.base_tpl') %}

{% block content %}
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h1>{{ title | e }}</h1>
                {% if traceback and reg_get('debug') %}
                    <pre>{{ traceback | e }}</pre>
                {% endif %}
            </div>
        </div>
    </div>
{% endblock %}
